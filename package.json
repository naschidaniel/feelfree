{
  "name": "fly-tirol",
  "version": "1.0.0",
  "private": true,
  "description": "fly-tirol.com paragliding school Westendorf",
  "author": "Daniel Naschberger",
  "precommit": "yarn lint",
  "scripts": {
    "serve": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "lint:js": "eslint --fix  --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "generateMediaInformation": "node generateMediaInformation.js",
    "optimize-images:2xs": "./image-optimizer ./static/media ./dist/media 2xs 384 95",
    "optimize-images:xs": "./image-optimizer ./static/media ./dist/media xs 512 90",
    "optimize-images:sm": "./image-optimizer ./static/media ./dist/media sm 640 80",
    "optimize-images:md": "./image-optimizer ./static/media ./dist/media md 768 75",
    "optimize-images:lg": "./image-optimizer ./static/media ./dist/media lg 1024 75",
    "optimize-images:xl": "./image-optimizer ./static/media ./dist/media xl 1280 75",
    "optimize-images:2xl": "./image-optimizer ./static/media ./dist/media 2xl 1536 75",
    "optimize-images": "yarn optimize-images:2xs && yarn optimize-images:xs && yarn optimize-images:sm && yarn optimize-images:md && yarn optimize-images:lg && yarn optimize-images:xl && yarn optimize-images:2xl"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.9.1",
    "heroicons": "^1.0.1",
    "nuxt": "^2.15.3",
    "nuxt-shopify": "^1.10.8"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "fibers": "^5.0.0",
    "glob": "^7.1.7",
    "image-size": "^1.0.0",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.1",
    "tailwindcss-debug-screens": "^2.0.0"
  }
}
