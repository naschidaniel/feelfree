{
  "name": "flying-in-tirol",
  "version": "2.0.0",
  "private": true,
  "description": "fly-tirol.com paragliding school Westendorf and white-cloud.tirol tandemflights",
  "author": "Daniel Naschberger",
  "precommit": "yarn lint",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "lint": "eslint --fix --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lintContent": "eslint --no-error-on-unmatched-pattern --fix --ext \".vue,.js,.ts\" content_flytirol content_whitecloud data",
    "generateContent": "node generateContent.js && yarn lintContent",
    "generateMediaInformation": "node generateMediaInformation.js && yarn lintContent",
    "create-public:flytirol": "rm -rf .public_flytirol && cp -r public_flytirol/ .public_flytirol",
    "optimize-images:flytirol": "yarn create-public:flytirol && ./image-optimizer --source public_flytirol/media --destination .public_flytirol/media --widths 384 512 640 768 1024 1280 1536 --qualities 95 90 80 75 75 75 75",
    "create-public:whitecloud": "rm -rf .public_whitecloud && cp -r public_whitecloud/ .public_whitecloud",
    "optimize-images:whitecloud": "yarn create-public:whitecloud && ./image-optimizer --source public_whitecloud/media --destination .public_whitecloud/media --widths 384 512 640 768 1024 1280 1536 --qualities 95 90 80 75 75 75 75",
    "optimize-images": "yarn optimize-images:flytirol && yarn optimize-images:whitecloud",
    "create-public": "yarn generateMediaInformation && yarn create-public:flytirol && yarn create-public:whitecloud",
    "buildDev": "yarn create-public && yarn optimize-images:flytirol && yarn generateContent"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.10.3",
    "core-js": "^3.35.0",
    "heroicons": "^2.1.1",
    "leaflet": "^1.9.4",
    "nuxt": "^3.9.0",
    "simple-icons": "^10.4.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.9.0",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.19.2",
    "glob": "^10.3.10",
    "image-size": "^1.1.1",
    "marked": "^11.1.1",
    "nitropack": "^2.8.1",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
