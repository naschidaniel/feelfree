{
  "name": "flying-in-tirol",
  "version": "2.0.0",
  "private": true,
  "description": "fly-tirol.com paragliding school Westendorf and white-cloud.tirol tandemflights",
  "author": "Daniel Naschberger",
  "precommit": "yarn lint",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "lint": "eslint --fix --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lintContent": "eslint --no-error-on-unmatched-pattern --fix --ext \".vue,.js,.ts\" ./content_flytirol ./content_whitecloud ./data",
    "generateContent": "node generateContent.js && yarn lintContent",
    "generateMediaInformation": "node generateMediaInformation.js && yarn lintContent",
    "create-public:flytirol": "rm -rf .public_flytirol && cp -r public_flytirol/ .public_flytirol",
    "optimize-images:flytirol:384": "./image-optimizer --source ./public_flytirol/media --destination ./.public_flytirol/media --suffix 384 --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:flytirol:512": "./image-optimizer --source ./public_flytirol/media --destination ./.public_flytirol/media --suffix 512 --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:flytirol:640": "./image-optimizer --source ./public_flytirol/media --destination ./.public_flytirol/media --suffix 640 --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:flytirol:768": "./image-optimizer --source ./public_flytirol/media --destination ./.public_flytirol/media --suffix 768 --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:1024": "./image-optimizer --source ./public_flytirol/media --destination ./.public_flytirol/media --suffix 1024 --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:1280": "./image-optimizer --source ./public_flytirol/media --destination ./.public_flytirol/media --suffix 1280 --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:1536": "./image-optimizer --source ./public_flytirol/media --destination ./.public_flytirol/media --suffix 1536 --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol": "yarn create-public:flytirol && yarn optimize-images:flytirol:384 && yarn optimize-images:flytirol:512 && yarn optimize-images:flytirol:640 && yarn optimize-images:flytirol:768 && yarn optimize-images:flytirol:1024 && yarn optimize-images:flytirol:1280 && yarn optimize-images:flytirol:1536",
    "create-public:whitecloud": "rm -rf .public_whitecloud && cp -r ./public_whitecloud/ ./.public_whitecloud",
    "optimize-images:whitecloud:384": "./image-optimizer --source ./public_whitecloud/media --destination ./.public_whitecloud/media --suffix 384 --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:512": "./image-optimizer --source ./public_whitecloud/media --destination ./.public_whitecloud/media --suffix 512 --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:640": "./image-optimizer --source ./public_whitecloud/media --destination ./.public_whitecloud/media --suffix 640 --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:768": "./image-optimizer --source ./public_whitecloud/media --destination ./.public_whitecloud/media --suffix 768 --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:1024": "./image-optimizer --source ./public_whitecloud/media --destination ./.public_whitecloud/media --suffix 1024 --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:1280": "./image-optimizer --source ./public_whitecloud/media --destination ./.public_whitecloud/media --suffix 1280 --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:1536": "./image-optimizer --source ./public_whitecloud/media --destination ./.public_whitecloud/media --suffix 1536 --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud": "yarn create-public:whitecloud && yarn optimize-images:whitecloud:384 && yarn optimize-images:whitecloud:512 && yarn optimize-images:whitecloud:640 && yarn optimize-images:whitecloud:768 && yarn optimize-images:whitecloud:1024 && yarn optimize-images:whitecloud:1280 && yarn optimize-images:whitecloud:1536",
    "optimize-images": "yarn optimize-images:flytirol && yarn optimize-images:whitecloud",
    "create-public": "yarn generateMediaInformation && yarn create-public:flytirol && yarn create-public:whitecloud && yarn optimize-images"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^5.3.1",
    "core-js": "^3.24.1",
    "heroicons": "^1.0.6",
    "leaflet": "^1.8.0",
    "nuxt": "npm:nuxt3@latest",
    "shopify-buy": "^2.16.1",
    "simple-icons": "^7.5.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.0.0-rc.6",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "glob": "^8.0.3",
    "image-size": "^1.0.2",
    "marked": "^4.0.18",
    "nitropack": "^0.4.12",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vue-tsc": "^0.39.2"
  }
}
