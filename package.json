{
  "name": "flying-in-tirol",
  "version": "2.0.0",
  "private": true,
  "description": "fly-tirol.com paragliding school Westendorf and white-cloud.tirol tandemflights",
  "author": "Daniel Naschberger",
  "precommit": "yarn lint",
  "type": "module",
  "scripts": {
    "dev": "nuxt --hostname '0.0.0.0'",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "lint": "eslint --fix --ext \".cjs,.js,.vue\" --ignore-path .gitignore .",
    "lintContent": "eslint --fix --ext \".vue\" ./content_flytirol ./content_whitecloud",
    "generateContent": "node generateContent.js && yarn lintContent",
    "generateMediaInformation": "node generateMediaInformation.js",
    "optimize-images:flytirol:2xs": "./image-optimizer --source ./static_flytirol/media --destination .nuxt/dist/media --suffix 2xs --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:flytirol:xs": "./image-optimizer --source ./static_flytirol/media --destination .nuxt/dist/media --suffix xs --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:flytirol:sm": "./image-optimizer --source ./static_flytirol/media --destination .nuxt/dist/media --suffix sm --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:flytirol:md": "./image-optimizer --source ./static_flytirol/media --destination .nuxt/dist/media --suffix md --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:lg": "./image-optimizer --source ./static_flytirol/media --destination .nuxt/dist/media --suffix lg --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:xl": "./image-optimizer --source ./static_flytirol/media --destination .nuxt/dist/media --suffix xl --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:2xl": "./image-optimizer --source ./static_flytirol/media --destination .nuxt/dist/media --suffix 2xl --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol": "yarn optimize-images:flytirol:2xs && yarn optimize-images:flytirol:xs && yarn optimize-images:flytirol:sm && yarn optimize-images:flytirol:md && yarn optimize-images:flytirol:lg && yarn optimize-images:flytirol:xl && yarn optimize-images:flytirol:2xl",
    "optimize-images:whitecloud:2xs": "./image-optimizer --source ./static_whitecloud/media --destination .nuxt/dist/media --suffix 2xs --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:xs": "./image-optimizer --source ./static_whitecloud/media --destination .nuxt/dist/media --suffix xs --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:sm": "./image-optimizer --source ./static_whitecloud/media --destination .nuxt/dist/media --suffix sm --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:md": "./image-optimizer --source ./static_whitecloud/media --destination .nuxt/dist/media --suffix md --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:lg": "./image-optimizer --source ./static_whitecloud/media --destination .nuxt/dist/media --suffix lg --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:xl": "./image-optimizer --source ./static_whitecloud/media --destination .nuxt/dist/media --suffix xl --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:2xl": "./image-optimizer --source ./static_whitecloud/media --destination .nuxt/dist/media --suffix 2xl --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud": "yarn optimize-images:whitecloud:2xs && yarn optimize-images:whitecloud:xs && yarn optimize-images:whitecloud:sm && yarn optimize-images:whitecloud:md && yarn optimize-images:whitecloud:lg && yarn optimize-images:whitecloud:xl && yarn optimize-images:whitecloud:2xl"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.33.0",
    "cookie-universal-nuxt": "^2.2.1",
    "core-js": "^3.23.3",
    "heroicons": "^1.0.6",
    "leaflet": "^1.8.0",
    "nuxt-edge": "latest",
    "shopify-buy": "^2.16.1",
    "simple-icons": "^7.4.0",
    "tailwindcss": "^3.1.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "glob": "^8.0.3",
    "image-size": "^1.0.1",
    "marked": "^4.0.17",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1"
  }
}
