{
  "name": "fly-tirol",
  "version": "1.0.0",
  "private": true,
  "description": "fly-tirol.com paragliding school Westendorf",
  "author": "Daniel Naschberger",
  "precommit": "yarn lint",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "lint": "eslint --fix  --ext \".js,.vue\" --ignore-path .gitignore .",
    "lintContent": "eslint --fix  --ext .vue ./content",
    "generateContent": "node generateContent.js",
    "generateMediaInformation": "node generateMediaInformation.js",
    "optimize-images:2xs": "./image-optimizer --source ./static/media --destination ./dist/media --suffix 2xs --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:xs": "./image-optimizer --source ./static/media --destination ./dist/media --suffix xs --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:sm": "./image-optimizer --source ./static/media --destination ./dist/media --suffix sm --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:md": "./image-optimizer --source ./static/media --destination ./dist/media --suffix md --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:lg": "./image-optimizer --source ./static/media --destination ./dist/media --suffix lg --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:xl": "./image-optimizer --source ./static/media --destination ./dist/media --suffix xl --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:2xl": "./image-optimizer --source ./static/media --destination ./dist/media --suffix 2xl --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images": "yarn optimize-images:2xs && yarn optimize-images:xs && yarn optimize-images:sm && yarn optimize-images:md && yarn optimize-images:lg && yarn optimize-images:xl && yarn optimize-images:2xl"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.29.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.9.1",
    "heroicons": "^1.0.1",
    "nuxt": "^2.15.3",
    "nuxt-shopify": "^1.10.8",
    "simple-icons": "^6.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@tailwindcss/jit": "^0.1.18",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "glob": "^7.1.7",
    "image-size": "^1.0.0",
    "marked": "^4.0.10",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1"
  }
}
