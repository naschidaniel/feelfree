{
  "name": "flying-in-tirol",
  "version": "2.0.0",
  "private": true,
  "description": "fly-tirol.com paragliding school Westendorf and white-cloud.tirol tandemflights",
  "author": "Daniel Naschberger",
  "precommit": "yarn lint",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "lint": "eslint --fix --ext \".cjs,.js,.vue\" --ignore-path .gitignore .",
    "lintContent": "eslint --no-error-on-unmatched-pattern --fix --ext \".vue,.js,.ts\" ./content_flytirol ./content_whitecloud ./data",
    "generateContent": "node --loader ts-node/esm generateContent.ts && yarn lintContent",
    "generateMediaInformation": "node --experimental-json-modules --loader ts-node/esm generateMediaInformation.ts && yarn lintContent",
    "optimize-images:flytirol:2xs": "./image-optimizer --source ./public_flytirol/media --destination .nuxt/dist/media --suffix 2xs --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:flytirol:xs": "./image-optimizer --source ./public_flytirol/media --destination .nuxt/dist/media --suffix xs --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:flytirol:sm": "./image-optimizer --source ./public_flytirol/media --destination .nuxt/dist/media --suffix sm --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:flytirol:md": "./image-optimizer --source ./public_flytirol/media --destination .nuxt/dist/media --suffix md --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:lg": "./image-optimizer --source ./public_flytirol/media --destination .nuxt/dist/media --suffix lg --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:xl": "./image-optimizer --source ./public_flytirol/media --destination .nuxt/dist/media --suffix xl --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol:2xl": "./image-optimizer --source ./public_flytirol/media --destination .nuxt/dist/media --suffix 2xl --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:flytirol": "yarn optimize-images:flytirol:2xs && yarn optimize-images:flytirol:xs && yarn optimize-images:flytirol:sm && yarn optimize-images:flytirol:md && yarn optimize-images:flytirol:lg && yarn optimize-images:flytirol:xl && yarn optimize-images:flytirol:2xl",
    "optimize-images:whitecloud:2xs": "./image-optimizer --source ./public_whitecloud/media --destination .nuxt/dist/media --suffix 2xs --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:xs": "./image-optimizer --source ./public_whitecloud/media --destination .nuxt/dist/media --suffix xs --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:sm": "./image-optimizer --source ./public_whitecloud/media --destination .nuxt/dist/media --suffix sm --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:md": "./image-optimizer --source ./public_whitecloud/media --destination .nuxt/dist/media --suffix md --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:lg": "./image-optimizer --source ./public_whitecloud/media --destination .nuxt/dist/media --suffix lg --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:xl": "./image-optimizer --source ./public_whitecloud/media --destination .nuxt/dist/media --suffix xl --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud:2xl": "./image-optimizer --source ./public_whitecloud/media --destination .nuxt/dist/media --suffix 2xl --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:whitecloud": "yarn optimize-images:whitecloud:2xs && yarn optimize-images:whitecloud:xs && yarn optimize-images:whitecloud:sm && yarn optimize-images:whitecloud:md && yarn optimize-images:whitecloud:lg && yarn optimize-images:whitecloud:xl && yarn optimize-images:whitecloud:2xl"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^5.3.0",
    "core-js": "^3.23.5",
    "heroicons": "^1.0.6",
    "leaflet": "^1.8.0",
    "nuxt": "npm:nuxt3@latest",
    "shopify-buy": "^2.16.1",
    "simple-icons": "^7.4.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@nuxt/kit": "^3.0.0-rc.6",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@types/shopify-buy": "^2.10.10",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "glob": "^8.0.3",
    "image-size": "^1.0.2",
    "marked": "^4.0.18",
    "nitropack": "^0.4.12",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vue-tsc": "^0.38.9"
  }
}
