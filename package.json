{
  "name": "fly-tirol",
  "version": "1.0.0",
  "private": true,
  "description": "fly-tirol.com paragliding school Westendorf",
  "author": "Daniel Naschberger",
  "precommit": "yarn lint",
  "scripts": {
    "dev": "nuxt --hostname '0.0.0.0'",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "lint": "eslint --fix  --ext \".js,.vue\" --ignore-path .gitignore .",
    "lintContent": "eslint --fix  --ext .vue ./content_flytirol ./content_whitecloud",
    "generateContent": "node generateContent.js",
    "generateMediaInformation": "node generateMediaInformation.js",
    "optimize-images:ft:2xs": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix 2xs --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:ft:xs": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix xs --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:ft:sm": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix sm --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:ft:md": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix md --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:ft:lg": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix lg --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:ft:xl": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix xl --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:ft:2xl": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix 2xl --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:ft": "yarn optimize-images:ft:2xs && yarn optimize-images:ft:xs && yarn optimize-images:ft:sm && yarn optimize-images:ft:md && yarn optimize-images:ft:lg && yarn optimize-images:ft:xl && yarn optimize-images:ft:2xl",
    "optimize-images:wc:2xs": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix 2xs --width 384 --quality 95 --webpimage true --thumbnail true",
    "optimize-images:wc:xs": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix xs --width 512 --quality 90 --webpimage true --thumbnail true",
    "optimize-images:wc:sm": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix sm --width 640 --quality 80 --webpimage true --thumbnail true",
    "optimize-images:wc:md": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix md --width 768 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:wc:lg": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix lg --width 1024 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:wc:xl": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix xl --width 1280 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:wc:2xl": "./image-optimizer --source ./static/media --destination .nuxt/dist/media --suffix 2xl --width 1536 --quality 75 --webpimage true --thumbnail true",
    "optimize-images:wc": "yarn optimize-images:wc:2xs && yarn optimize-images:wc:xs && yarn optimize-images:wc:sm && yarn optimize-images:wc:md && yarn optimize-images:wc:lg && yarn optimize-images:wc:xl && yarn optimize-images:wc:2xl"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.29.3",
    "cookie-universal-nuxt": "^2.1.5",
    "heroicons": "^1.0.1",
    "nuxt": "^2.15.3",
    "nuxt-shopify": "^1.10.8",
    "simple-icons": "^6.0.0"
  },
  "devDependencies": {
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "glob": "^7.1.7",
    "image-size": "^1.0.0",
    "marked": "^4.0.10",
    "postcss": "^8.4.6",
    "prettier": "^2.2.1",
    "tailwindcss": "^3.0.23"
  }
}
